$TTL 604800
@ IN SOA n1.local. root.localhost. (
         3;          Serial
         604800;     Refresh
         86400;      Retry
         2419200;    Expire
         604800 );   Negative Cache TTL

; define nameservers
                IN NS      n1
                IN NS      n2 
; nameserver ip addresses
n1              IN A       192.168.10.150
n2              IN A       192.168.10.151

; roundrobin service
service         IN A       192.168.10.150 
                IN A       192.168.10.151 

